pipeline {
    agent any
    stages {
        stage('Stage 1') {
            steps {
                echo 'Hello, World!'
            }
        }
        stage('Stage 2') {
            steps {
                echo 'Doing some work...'
            }
        }
        stage('Stage 3') {
            steps {
                echo 'Goodbye, World!'
            }
        }
    }
}
